<Project Sdk="Godot.NET.Sdk/4.1.0-dev.4">
    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <EnableDynamicLoading>true</EnableDynamicLoading>
        <Nullable>enable</Nullable>
        <LangVersion>latest</LangVersion>
        <AssemblyName>Spaghetti</AssemblyName>
        <RootNamespace>Spaghetti</RootNamespace>
    </PropertyGroup>
    <ItemGroup>
        <Compile Remove="Debug\**" />
        <Compile Remove="SourceGenerators\**" />
    </ItemGroup>
    <ItemGroup>
        <EmbeddedResource Remove="Debug\**" />
        <EmbeddedResource Remove="SourceGenerators\**" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="Firebelley.GodotUtilities" Version="4.0.4" />
        <PackageReference Include="Firebelley.GodotUtilities.SourceGenerators" Version="4.0.4" />
        <PackageReference Include="JetBrains.Annotations" Version="2022.3.1" />
        <PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="4.6.0" />
    </ItemGroup>
    <ItemGroup>
        <Content Include="icon.svg" />
        <Content Include="m5x7.ttf" />
    </ItemGroup>
</Project>